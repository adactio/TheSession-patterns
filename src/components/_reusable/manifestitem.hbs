<li class="manifest-item">
{{#if title}}
<a href="#"><span class="manifest-item-title">{{ title }}{{#if detail}} <span class="detail">{{ detail }}</span>{{/if}}</span></a>
{{/if}}
{{#if content}}
<div>{{ content }}</div>
{{/if}}
{{#if meta}}
<small class="manifest-meta discrete">
{{ meta }}
</small>
{{/if}}
</li>
